{
	"manifest_version": 2,
	"name": "Helloworld",
	"description": "This is Helloworld chrome extension",
	"version": "1",
	"permissions": ["tabs", "http://*/*", "background"],
  "browser_action": {
    "default_icon": "16x16.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' http://localhost; object-src 'self'"
}

