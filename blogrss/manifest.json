{
  "manifest_version": 2,
  "name": "Lastest News eXo's Blog",
  "description": "This extension get lastest news from eXo's Blog",
  "version": "1.0",
  "browser_action": {
    "default_title": "Lastest News",
    "default_icon": "icon.png",
    "default_popup": "lastestnews.html"
  },
  "permissions": [
    "http://blog.exoplatform.com/*"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}

